<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.dhu.test4service.pojo.User">
        <table name="user" schema="Test4" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id"/>
            </id>
            <basic name="name">
                <column name="name"/>
            </basic>
            <basic name="password">
                <column name="password"/>
            </basic>
            <basic name="role">
                <column name="role"/>
            </basic>
            <basic name="tel">
                <column name="tel"/>
            </basic>
            <basic name="mailbox">
                <column name="mailbox"/>
            </basic>
            <basic name="address">
                <column name="address"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
